
public class Antworten {
	public int teilnehmer;
	public int ja;
	public int nein;

	public int getJa() {
		return ja;
	}

	public void setJa(int ja) {
		this.ja = ja;
	}

	public int getNein() {
		return nein;
	}

	public void setNein(int nein) {
		this.nein = nein;
	}

	public int getTeilnehmer() {
		return teilnehmer;
	}

	public void setTeilnehmer(int teilnehmer) {
		this.teilnehmer = teilnehmer;
	}

}
