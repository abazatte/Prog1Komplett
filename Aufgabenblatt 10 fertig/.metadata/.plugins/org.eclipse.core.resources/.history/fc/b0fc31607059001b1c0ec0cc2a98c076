
public class Test {
	public static void main (String [] args) {
	Konto p0 = new Konto();
	Konto p1 = new Konto(p0);
	p0.einZahlung();
	System.out.println("Kontostand Person 1"+Konto.kontoStandAusgeben(p0));
	Konto.Ueberweisung(p0, p1);
	System.out.println(Konto.kontoStandAusgeben(p0));
	System.out.println(Konto.kontoStandAusgeben(p1));
	Konto p1Klon = p1.klonier();
	System.out.println(Konto.kontoStandAusgeben(p1Klon));
	}
}
